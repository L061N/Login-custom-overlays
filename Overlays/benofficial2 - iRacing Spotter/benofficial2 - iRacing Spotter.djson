{
  "Variables": {
    "DashboardVariables": [
      {
        "VariableName": "monitorVisible",
        "EvaluateOnlyOnce": false,
        "OverrideWithParentDashboardVariableWhenAvailable": true,
        "ValueExpression": {
          "JSExt": 0,
          "Interpreter": 1,
          "Expression": "// In iRacing SDK we don't have the data for opponents car position, so we can't tell if the car is left or right. Only the spotter gives us that information, but it wouldn't trigger with every overlap (ex: on the pace lap).\r\nconst behindDist0 = $prop('IRacingExtraProperties.iRacing_DriverBehind_00_Distance');\r\nconst behindDist1 = $prop('IRacingExtraProperties.iRacing_DriverBehind_01_Distance');\r\nconst aheadDist0 = $prop('IRacingExtraProperties.iRacing_DriverAhead_00_Distance');\r\nconst aheadDist1 = $prop('IRacingExtraProperties.iRacing_DriverAhead_01_Distance');\r\nconst spotterThreshold = isnull($prop('benofficial2.Spotter.Threshold'), 5.5);\r\n\r\nconst behind = (behindDist0 > 0 && behindDist0 <= spotterThreshold) || \r\n               (behindDist1 > 0 && behindDist1 <= spotterThreshold);\r\n\r\nconst ahead = (aheadDist0 < 0 && aheadDist0 >= -spotterThreshold) || \r\n              (aheadDist1 < 0 && aheadDist1 >= -spotterThreshold);\r\n\r\nreturn ahead || behind;"
        },
        "EvaluateBeforeScreenRoles": false
      },
      {
        "VariableName": "overlapBehind",
        "EvaluateOnlyOnce": false,
        "OverrideWithParentDashboardVariableWhenAvailable": true,
        "ValueExpression": {
          "JSExt": 1,
          "Interpreter": 1,
          "Expression": "const behindDist0 = $prop('IRacingExtraProperties.iRacing_DriverBehind_00_Distance');\r\nconst behindDist1 = $prop('IRacingExtraProperties.iRacing_DriverBehind_01_Distance');\r\nconst spotterThreshold = isnull($prop('benofficial2.Spotter.Threshold'), 5.5);\r\nlet behind = 0\r\n\r\nif (behindDist0 > 0 && behindDist0 <= spotterThreshold)\r\n{\r\n\tbehind = behindDist0;\r\n}\r\n\r\nif (behindDist1 > 0 && behindDist1 <= spotterThreshold)\r\n{\r\n\tbehind = Math.max(behind, behindDist1);\r\n}\r\n\r\nreturn behind;"
        },
        "EvaluateBeforeScreenRoles": false
      },
      {
        "VariableName": "overlapAhead",
        "EvaluateOnlyOnce": false,
        "OverrideWithParentDashboardVariableWhenAvailable": true,
        "ValueExpression": {
          "JSExt": 0,
          "Interpreter": 1,
          "Expression": "const aheadDist0 = $prop('IRacingExtraProperties.iRacing_DriverAhead_00_Distance');\r\nconst aheadDist1 = $prop('IRacingExtraProperties.iRacing_DriverAhead_01_Distance');\r\nconst spotterThreshold = isnull($prop('benofficial2.Spotter.Threshold'), 5.5);\r\nlet ahead = 0;\r\n\r\nif (aheadDist0 < 0 && aheadDist0 >= -spotterThreshold)\r\n{\r\n\tahead = aheadDist0;\r\n}\r\n\r\nif (aheadDist1 < 0 && aheadDist1 >= -spotterThreshold)\r\n{\r\n\tahead = Math.min(ahead, aheadDist1);\r\n}\r\n\r\nreturn ahead;"
        },
        "EvaluateBeforeScreenRoles": false
      },
      {
        "VariableName": "spotterHeight",
        "EvaluateOnlyOnce": false,
        "OverrideWithParentDashboardVariableWhenAvailable": true,
        "ValueExpression": {
          "JSExt": 0,
          "Interpreter": 1,
          "Expression": "const spotterThreshold = isnull($prop('benofficial2.Spotter.Threshold'), 5.5);\r\nconst spotterHeight = isnull($prop('benofficial2.Spotter.Height'), 129);\r\nconst spotterMinHeight = isnull($prop('benofficial2.Spotter.MinHeight'), 15);\r\n\r\nif ($prop('variable.overlapAhead') < 0 && $prop('variable.overlapBehind') > 0)\r\n{\r\n\treturn spotterHeight;\r\n}\r\n\r\nif ($prop('variable.overlapBehind') > 0)\r\n{\r\n\treturn spotterHeight - (spotterHeight - spotterMinHeight) * $prop('variable.overlapBehind') / spotterThreshold;\r\n}\r\n\r\nif ($prop('variable.overlapAhead') < 0)\r\n{\r\n\treturn spotterHeight + (spotterHeight - spotterMinHeight) * $prop('variable.overlapAhead') / spotterThreshold;\r\n}\r\n\r\nreturn 0;"
        },
        "EvaluateBeforeScreenRoles": false
      },
      {
        "VariableName": "spotterTop",
        "EvaluateOnlyOnce": false,
        "OverrideWithParentDashboardVariableWhenAvailable": true,
        "ValueExpression": {
          "JSExt": 0,
          "Interpreter": 1,
          "Expression": "const spotterThreshold = isnull($prop('benofficial2.Spotter.Threshold'), 5.5);\r\nconst spotterBorder = isnull($prop('benofficial2.Spotter.Border'), 3);\r\nconst spotterHeight = isnull($prop('benofficial2.Spotter.Height'), 129);\r\nconst spotterMinHeight = isnull($prop('benofficial2.Spotter.MinHeight'), 15);\r\n\r\nif ($prop('variable.overlapAhead') < 0 && $prop('variable.overlapBehind') > 0)\r\n{\r\n\treturn spotterBorder;\r\n}\r\n\r\nif ($prop('variable.overlapBehind') > 0)\r\n{\r\n\treturn spotterBorder + (spotterHeight - spotterMinHeight) * $prop('variable.overlapBehind') / spotterThreshold;\r\n}\r\n\r\nreturn spotterBorder;"
        },
        "EvaluateBeforeScreenRoles": false
      },
      {
